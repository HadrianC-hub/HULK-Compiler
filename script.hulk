type Vehicle {
    move() => "Moving generically";
}

type Car inherits Vehicle {
    move() => base() @@ " with wheels";
}

type Bike inherits Vehicle {
    move() => base() @@ " with two wheels";
}

function test (x)
{
    if (x == 0) 
    {
        new Car();
    }
        else 
    {
        new Bike();
    };
}

let i = test(1) in print(i.move());